general {
	output_format = "i3bar"
}

order += "disk /"
order += memory
order += "volume default"
order += time

disk / {
	format = %avail
}

memory {
	format = %available
}

time {
	format = "%H:%M %a %d"
}

volume default {
	format = "ðŸ”Š %volume"
	format_muted = "ðŸ”ˆ(%volume)"
	align = center
}
