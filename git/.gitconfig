[user]
	email = masondeanm@aol.com
	name = Mason Mackaman
[core]
	pager = less -x1,5
	editor = vim
[alias]
	graph-all = log --oneline --abbrev=0 --graph --all
	first-push = push -u origin master
	ga = graph-all
	cloners = clone --recurse-submodules
	tpush = push --tags origin master
[winUpdater]
	recentlySeenVersion = 2.25.1.windows.1
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
